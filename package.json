{
  "name": "integrated_healthcare_system",
  "version": "1.0.5",
  "description": "healthcare system",
  "bin": "./bin/script.js",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wubeZ/create-express-ts-starter.git"
  },
  "scripts": {
    "dev": "nodemon src/server.js | npx pino-pretty",
    "start": "node dist/src/server.js | npx pino-pretty",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "debug-test": "jest --runInBand --detectOpenHandles",
    "debug-test-coverage": "jest --runInBand --detectOpenHandles --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "prettier-watch": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
    "prettier:fix": "prettier --write src/**/*.js",
    "prepare": "husky"
  },
  "keywords": [
    "javascript",
    "express",
    "starter",
    "boilerplate",
    "mongoose",
    "mongodb",
    "atlas",
    "pino",
    "jest",
    "eslint",
    "prettier",
    "husky",
    "nodemon",
    "supertest"
  ],
  "author": "Wubshet Zeleke",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.3",
    "pino": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "onchange": "^7.1.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  }
}
